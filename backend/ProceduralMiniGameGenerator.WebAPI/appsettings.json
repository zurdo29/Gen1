{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Hangfire": "Information"
    },
    "PerformanceThresholds": {
      "Default": "2000",
      "Generation": "5000",
      "Export": "3000",
      "Validation": "1000"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "PluginSettings": {
    "Directory": "plugins",
    "EnableExternalPlugins": true,
    "LoadBuiltInPlugins": true
  },
  "AllowedHosts": "*",
  "CorsOrigins": [
    "http://localhost:3000",
    "http://localhost:5173"
  ]
}