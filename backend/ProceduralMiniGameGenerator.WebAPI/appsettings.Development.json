{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    },
    "PerformanceThresholds": {
      "Default": "1000",
      "Generation": "3000",
      "Export": "2000",
      "Validation": "500"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    }
  },
  "PluginSettings": {
    "Directory": "plugins",
    "EnableExternalPlugins": true,
    "LoadBuiltInPlugins": true
  }
}